{%- if section.settings.fullwidth -%}
	{%- assign container = 'container-full' -%}
{%- elsif section.settings.max_width != blank -%}
	{%- assign container = 'bwp-container' -%}
{%- else -%}
	{%- assign container = 'container' -%}
{%- endif -%}

{%- assign max_width = '' -%}
{%- if section.settings.fullwidth == blank and section.settings.max_width != blank -%}
	{%- assign width_container = section.settings.max_width | plus: 0 -%}
	{%- capture max_width -%}style="max-width: {{ section.settings.max_width }};"{%- endcapture -%}
{%- endif -%}

{% assign margin 	= section.settings.margin | split: "|" %}
{% assign padding 	= section.settings.padding | split: "|" %}
{% assign padding_bg 	= section.settings.padding_bg | split: "|" %}
{%- style -%}
	{%- if section.settings.background -%}
		{%- assign background = section.settings.background | img_url: "" -%}
		#shopify-section-{{ section.id }} .kretoss-section__inner{
			background-image: url('{{ background }}');
		}
	{%- else -%}
		#shopify-section-{{ section.id }} .kretoss-section__inner{
			background-image: url('https://placehold.jp/1410x1054.png');
		}
	{%- endif -%}
	#shopify-section-{{ section.id }} .kretoss-section__inner{
		background-size:{{ section.settings.background_size }};
		background-position:{{ section.settings.background_pos }};
		{%- if section.settings.bg_fixed != blank -%}
			background-attachment: fixed;
		{%- endif -%}
		{%- if padding_bg[0] != blank -%}padding:{{ padding_bg[0] }};{%- endif -%}
	}
	{%- if margin[0] != blank or padding[0] != blank -%}
		#shopify-section-{{ section.id }} .kretoss-section--background{
			{%- if margin[0] != blank -%}margin: {{ margin[0] }};{%- endif -%}
			{%- if padding[0] != blank -%}padding:{{ padding[0] }};{%- endif -%}
		}
	{%- endif -%}
	{%- if margin[1] != blank or padding[1] != blank or padding_bg[1] != blank -%}
		@media (max-width:1200px){
			#shopify-section-{{ section.id }} .kretoss-section--background{
				{%- if margin[1] != blank -%}margin: {{ margin[1] }};{%- endif -%}
				{%- if padding[1] != blank -%}padding:{{ padding[1] }};{%- endif -%}
			}
			{%- if padding_bg[1] != blank -%}
			#shopify-section-{{ section.id }} .kretoss-section__inner{
				padding:{{ padding_bg[1] }};
			}
			{%- endif -%}
		}
	{%- endif -%}
	{%- if margin[2] != blank or padding[2] != blank or padding_bg[1] != blank -%}
		@media (max-width:991px){
			#shopify-section-{{ section.id }} .kretoss-section--background{
				{%- if margin[2] != blank -%}margin: {{ margin[2] }};{%- endif -%}
				{%- if padding[2] != blank -%}padding:{{ padding[2] }};{%- endif -%}
			}
			{%- if padding_bg[2] != blank -%}
			#shopify-section-{{ section.id }} .kretoss-section__inner{
				padding:{{ padding_bg[2] }};
			}
			{%- endif -%}
		}
	{%- endif -%}
	{%- if margin[3] != blank or padding[3] != blank or padding_bg[1] != blank -%}
		@media (max-width:767px){
			#shopify-section-{{ section.id }} .kretoss-section--background{
				{%- if margin[3] != blank -%}margin: {{ margin[3] }};{%- endif -%}
				{%- if padding[3] != blank -%}padding:{{ padding[3] }};{%- endif -%}
			}
			#shopify-section-{{ section.id }} .kretoss-section__inner{
				{%- if padding_bg[3] != blank -%}padding:{{ padding_bg[3] }};{%- endif -%}
			}
			
		}
	{%- endif -%}
	{%- for block in section.blocks -%}
		{%- case block.type -%}
			{%- when 'image' -%}
				#shopify-section-{{ section.id }} .kretoss-section--background .image-{{forloop.index}}{
					max-width:{{ block.settings.font_size }}px;
					padding:{{ block.settings.margin }};
					margin:{{ block.settings.align }};
				}
			{%- when 'title' -%}
				#shopify-section-{{ section.id }} .kretoss-section--background .heading-{{forloop.index}}{
					font-size:{{ block.settings.font_size }}px;
					line-height:{{ block.settings.line_height }}px;
					font-weight:{{ block.settings.font_weight }};
					letter-spacing:{{ block.settings.letter_spacing }}px;
					margin:{{ block.settings.margin }};
					color:{{ block.settings.color }};
					text-align:{{ block.settings.align }};
				}
			{%- when 'sub_title' -%}
				#shopify-section-{{ section.id }} .kretoss-section--background .subheading-{{forloop.index}}{
					font-size:{{ block.settings.font_size }}px;
					line-height:{{ block.settings.line_height }}px;
					font-weight:{{ block.settings.font_weight }};
					letter-spacing:{{ block.settings.letter_spacing }}px;
					margin:{{ block.settings.margin }};
					color:{{ block.settings.color }};
					text-align:{{ block.settings.align }};
				}
			{%- when 'descrioption' -%}
				#shopify-section-{{ section.id }} .kretoss-section--background .descrioption-{{forloop.index}}{
					font-size:{{ block.settings.font_size }}px;
					line-height:{{ block.settings.line_height }}px;
					font-weight:{{ block.settings.font_weight }};
					letter-spacing:{{ block.settings.letter_spacing }}px;
					color:{{ block.settings.color }};
					margin:{{ block.settings.margin }};
					text-align:{{ block.settings.align }};
				}
			{%- when 'button' -%}
				#shopify-section-{{ section.id }} .button-{{forloop.index}}{
					font-size:{{ block.settings.font_size }}px;
					line-height:{{ block.settings.line_height }}px;
					font-weight:{{ block.settings.font_weight }};
					letter-spacing:{{ block.settings.letter_spacing }}px;
					color:{{ block.settings.color }};
					background:{{ block.settings.background }};
					padding:{{ block.settings.padding }};
					margin:{{ block.settings.margin }};
					{%- if block.settings.border -%}
						border:{{ block.settings.border_width }} solid {{ block.settings.border_color }};
						border-radius:{{ block.settings.border_radius }}
					{%- endif -%}
				}
				#shopify-section-{{ section.id }} .button-{{forloop.index}}:hover{
					color:{{ block.settings.color_hover }};
					background:{{ block.settings.background_hover }};
				}
		{%- endcase -%}

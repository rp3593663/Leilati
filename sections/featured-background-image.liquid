						{%- endif -%}
					}
			{%- endcase -%}
		{%- endfor -%}
	}
{%- endstyle -%}

<section class="kretoss-section kretoss-section--background kretoss-section--{{ section.id }} overflow-hidden">
	<div class="{{ container }}" {{ max_width }}>
		<div class="kretoss-section__inner">
			{%- for block in section.blocks -%}
				{%- case block.type -%}
					{%- when 'image' -%}
						<div class="kretoss-image image-{{forloop.index}}">
							{%- if block.settings.image -%}
								{% include 'img-global' with image: block.settings.image, class: "featured-banner__img", effect: "fade-in" %}
							{%- else -%}
								<img class="lazyload fade-in" src="https://placehold.jp/101x101.png" />
							{%- endif -%}
						</div>
					{%- when 'title' -%}
						{%- unless block.settings.title == blank -%}
							<h2 class="kretoss-title heading-{{forloop.index}}">
								{{ block.settings.title }}
							</h2>
						{%- endunless -%}
					{%- when 'sub_title' -%}
						{%- unless block.settings.title == blank -%}
							<div class="kretoss-sub-title subheading-{{forloop.index}}">
								{{ block.settings.title }}
							</div>
						{%- endunless -%}
					{%- when 'descrioption' -%}
						{%- unless block.settings.title == blank -%}
							<div class="wpbing-descrioption  descrioption-{{forloop.index}}" >
								{{ block.settings.title }}
							</div>
						{%- endunless -%}
					{%- when 'button' -%}
						{%- unless block.settings.title == blank -%}
							<a class="wpbing-button button-{{forloop.index}}" href="{{ block.settings.button_link }}" >
								{{ block.settings.title }}
							</a>
						{%- endunless -%}
				{%- endcase -%}
			{%- endfor -%}
		</div>
	</div>
</section>


	a {
		display: block;
		padding: 5px 22px;
		color: $menu-dropdown-color;
		font-size: $font-size;
		text-decoration: none;
		transition: color .35s ease;
		&:hover,
		&:focus {
			color: $menu-dropdown-hover-color;
		}

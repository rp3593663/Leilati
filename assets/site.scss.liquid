// MIXIN /////////////////////////////
@mixin rtl-margin-left($value){
	{% if settings.enable_rtl %} 
		margin-right: $value;
	{% else %}
		margin-left: $value;
	{% endif %}
}
@mixin rtl-margin-right($value){
	{% if settings.enable_rtl %} 
		margin-left: $value;
	{% else %}
		margin-right: $value;
	{% endif %}
}
@mixin rtl-margin($t, $r, $b, $l){
	{% if settings.enable_rtl %} 
		margin: $t $l $b $r;
	{% else %}
		margin: $t $r $b $l;
	{% endif %}
}
@mixin rtl-padding-left($value){
	{% if settings.enable_rtl %} 
		padding-right: $value;
	{% else %}
		padding-left: $value;
	{% endif %}
}
@mixin rtl-padding-right($value){

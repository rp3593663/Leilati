	&.active{
		opacity:1;
		visibility:visible;
		&.slide_inup{
			@include transform(translateY(0));
		}
		&.left_to_right{
			@include transform(translateX(0));
		}
		&.popup{
			@include transform(scale(1));
		}
		.scroll-notification{
			animation-name: notification-progressbar;
